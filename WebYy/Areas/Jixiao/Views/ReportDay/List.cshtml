                                <td>@item.new_num</td>
                                <td>@item.amount_1</td>
                                <td>@item.num_2</td>
                                <td>@item.contact_num</td>
                                <td>@item.amount_2</td>
                                <td>@item.old_amount</td>
                                <td>@item.datou_num</td>
                                <td>@item.amount</td>
