                        */
                var sum = DoMySql.FindField<ModelDb.p_jixiao_day>($"sum(new_num),sum(contact_num),sum(amount_1),sum(num_2),sum(amount_2),sum(old_amount),sum(datou_num),sum(amount)", $"c_date='{ViewBag.c_date}' and tg_user_sn='{ViewBag.tg_user_sn}'");
